{
  "version": 2,
  "builds": [
    {
      "src": "api/suggestions.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/suggestions.js"
    }
  ],
  "functions": {
    "api/suggestions.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
